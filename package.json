{
  "name": "fixdesk-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm:dev:vite\" \"npm:dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "tsc -p tsconfig.electron.json && wait-on tcp:5173 && electron .",
    "build": "npm run build:vite && tsc -p tsconfig.electron.json && electron-builder",
    "build:vite": "vite build",
    "preview": "vite preview",
    "postinstall": "electron-rebuild"
  },
  "dependencies": {
    "@google/genai": "latest",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "recharts": "^3.1.2",
    "robotjs": "^0.6.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.2",
    "concurrently": "^9.2.1",
    "electron": "^38.1.0",
    "electron-builder": "^26.0.12",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "wait-on": "^8.0.4"
  }
}
